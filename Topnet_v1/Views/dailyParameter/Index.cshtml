    @*@using (Html.BeginForm("Dashboard", "dailyParameter", FormMethod.Post, new { id = "userName", @class = "" }))
    {
        @Html.AntiForgeryToken()
        <button type="submit" class="btn btn-block btn-info btn-lg">Info</button>
