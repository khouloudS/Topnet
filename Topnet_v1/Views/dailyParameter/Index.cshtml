    </div>

<div class="box">
    <div>
        <h3 class="box-title">Historique KPI d'une ligne</h3> 
    </div>

    <!-- /.box-header -->
    <div class="box-body">


        <table id="example1" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>
                       Login
                    </th>

                    <th>
                       Date
                    </th>
                    <th>
                        Nombre de déconnexion par jour
                    </th>
                    <th>
                       Duree de session par jour
                    </th>
                    <th>
                        Duréé de déconnexion par jour
                    </th>
                    <th>
                        Gouvernorat
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.userName)
                    </td>

                    <td>
                        @Html.DisplayFor(modelItem => item.date)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.nbDeconnexionParJour)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.dureeConnexionParJour)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.dureeDeconnexionParJour)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.gouvernerat)
                    </td>
                </tr>
                }


            </tbody>
        </table>
    </div>

</div>


@section Scripts {
    @Scripts.Render("~/AdminLTE/plugins/datatables/js")
    @Scripts.Render("~/Scripts/Tables/Data")

}

